                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "qualityOnly" ;
                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                          rdf:first "ratedCriteria" ;
                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                     rdf:first "singleBidOnly" ;
                                                                                                                     rdf:rest rdf:nil
                                                                                                                   ]
                                                                                                        ]
                                                                                             ]
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                      ]
                          ] ;
